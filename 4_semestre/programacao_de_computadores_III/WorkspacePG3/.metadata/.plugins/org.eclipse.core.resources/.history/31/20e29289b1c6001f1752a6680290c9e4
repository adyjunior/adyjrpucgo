					cout<<endl<<"MATRIZ B : "<<endl;
					this->escreverMatriz(MB);
					cout<<endl<<endl;
					if(MA != MB)
						cout<<"MATRIZES SAO DIFERENTES"<<endl;
					else
						cout<<"MATRIZES NAO SAO DIFERENTES"<<endl;
					cout<<endl<<endl;
					break;
				}catch(int valorErro){
					cout<<endl<<endl<<"ERRO ("<<valorErro<<") -> "<<erro->getErro(valorErro);
				}

			}
			case 6:{
