		cout<<"                              \xba  08 - SIMETRICA                  \xba"<<endl;
		cout<<"                              \xba  09 - ANTI-SIMETRICA             \xba"<<endl;
		cout<<"                              \xba  10 - IDENTIDADE                 \xba"<<endl;
