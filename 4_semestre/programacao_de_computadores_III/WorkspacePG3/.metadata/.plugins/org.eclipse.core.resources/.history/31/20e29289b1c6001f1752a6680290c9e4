	int select=99;
	while(select){

		system("pause");
		system("cls");
		cout<<endl<<endl<<endl<<endl;
		cout<<"                              \xC9\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xcd\xBB"<<endl;
