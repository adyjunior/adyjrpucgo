	cout<<"Digite o numero de colunas da matriz B : ";
	cin>>colunaB;
	PC3::Matriz MB(linhaB,colunaB);
	this->lerMatriz(MB);
*/


	std::string nomeArquivo;
	cout<<"RECUPERAR MATRIZ (A)"<<endl<<endl;
